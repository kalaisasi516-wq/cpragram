#include <stdio.h>

#define MAX 50

int adj[MAX][MAX];
int visited[MAX];
int n;

void bfs(int s)
{
    int queue[MAX], front = 0, rear = -1;
    int i;

    for (i = 0; i < n; i++)
        visited[i] = 0;

    visited[s] = 1;
    queue[++rear] = s;

    printf("BFS Traversal: ");
    while (front <= rear)
    {
        int v = queue[front++];
        printf("%d ", v);

        for (i = 0; i < n; i++)
        {
            if (adj[v][i] == 1 && !visited[i])
            {
                visited[i] = 1;
                queue[++rear] = i;
            }
        }
    }
    printf("\n");
}
void dfs(int v)
{
    int i;
    visited[v] = 1;
    printf("%d ", v);

    for (i = 0; i < n; i++)
    {
        if (adj[v][i] == 1 && !visited[i])
            dfs(i);
    }
}

int main()
{
    int i, j;
    int A, B, S;

    printf("Enter number of cities: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 0; i < n; i++)
        for (j = 0; j < n; j++)
            scanf("%d", &adj[i][j]);

    printf("Enter cities A and B: ");
    scanf("%d %d", &A, &B);

    if (adj[A][B] == 1)
        printf("Connected\n");
    else
        printf("Not Connected\n");

    printf("Enter source vertex S: ");
    scanf("%d", &S);

    bfs(S);

    for (i = 0; i < n; i++)
        visited[i] = 0;

    printf("DFS Traversal: ");
    dfs(S);
    printf("\n");

    return 0;
}
